{
  "name": "Ofir-Videos-Games-World",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "cleandev": "rm -rf node_modules && npm i && npm start",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "deploy": "npm i angular-cli-ghpages --save-dev && ng build --base-href \"https://ofirdahan750.github.io/Ofir-Videos-Games-World/\" && npx angular-cli-ghpages --dir=docs",
    "push": "git add . && git commit -m \"done for now, works\" && git push",
    "fullpush": "npm run push && npm run deploy"
  },
  "private": true,
